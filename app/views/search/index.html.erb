<h1>Search Results</h1>
<h3> <%= @stores.count %> Total Stores</h3>

<% @stores.each do |store| %>
  <ul class="store">
    <li class="long_name">Long name: <%= store.long_name %></li>
    <li class="city">City: <%= store.city %></li>
    <li class="distance">Distance: <%= store.distance %></li>
    <li class="phone_number">Phone number: <%= store.phone_number %></li>
    <li class="store_type">Store type: <%= store.store_type %></li>
  </ul>
<% end %>
